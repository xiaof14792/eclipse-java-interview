package l;
/**
 * 控制任务组
 * @author 14792
 *
 */
public class ExecutorCompletionTest {
	public static void main(String[] args) {
		Runnable r1 = () ->{
			try {
				Thread.sleep(2000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
			System.out.println("第一个线程");
		};
	}
}
